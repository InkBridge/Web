  <script>
    $(document).ready(function () {
      $('.close').on('click', function () {
        $(this).parent('.alert').hide();
      });
    });
    var clicks = 0;
    $(document).ready(function () {
      $("#like").on({
        click: function () {
          if (clicks % 2 == 0) {
            $(this).css("-webkit-text-fill-color", "red");
            $(this).css("-webkit-text-stroke-color", "red");
            clicks = 1;
          } else {
            $(this).css("-webkit-text-fill-color", "white");
            $(this).css("-webkit-text-stroke-color", "black");
            clicks = 0;
          }

        }
      });

    });
  </script>





