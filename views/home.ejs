  @import url("https://fonts.googleapis.com/css?family=Roboto:100,300,400,500");

      .containernotif {
          position: relative;
          width: 100%;
          display: flex;
          flex-direction: column;
          justify-content: center;
          align-items: center;
          padding: 10px;
      }

      .titlenotif {
          width: 100%;
          font-size: 15px;
          letter-spacing: 4px;
          font-weight: 300;
          text-transform: uppercase;
          background: rgb(19, 18, 18);
          text-align: center;
          padding: 3% 10px;
          margin: 5px;
          color: white;
          position: relative;
          display: flex;
          justify-content: center;
          margin-top:10px;
      }

      .titlenotif:before {
          position: absolute;
          top: -10px;
          content: '';
          border-bottom: 12px solid rgb(17, 16, 16);
          border-left: 11px solid transparent;
          border-right: 11px solid transparent;
      }

      .icon {
          background: rgb(0, 0, 0);
          color: white;
          padding: 8px;
          border-radius: 50%;
          font-size: 13px;
      }

      .notification {
          margin: 2px;
          display: flex;
          align-items: center;
          justify-content: space-between;
          background: white;
          width: 100%;
          padding-left:5px;
          border-radius: 2%;
      }

      .notification .alert {
          display: flex;
          align-items: center;
          color: #555;
          text-transform: uppercase;
          font-size: 14px;
          padding: 0px;
          margin-left: 5px;
      }

      .notification .alert-name {
          font-weight: 500;
          margin-left: 10px;
      }

      .notification .alert-time {
          padding: 10px;
          font-size: 11px;
          letter-spacing: 1px;
          color: #555;
      }

      @media (max-width: 700px) {
          .containernotif {
              display: block;
              width: 100%;
          }

          .notification,
          .titlenotif {
              box-sizing: border-box;
          }

          .notification,
          .titlenotif {
